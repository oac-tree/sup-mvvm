#  SUP_MVVM_INCLUDE_DIRS - include directories for qt-mvvm
#  SUP_MVVM_LIBRARIES    - libraries to link against

get_filename_component(SUP_MVVM_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)

include(CMakeFindDependencyMacro)

find_dependency(QT NAMES Qt6 Qt5 REQUIRED COMPONENTS Widgets Core Gui PrintSupport)
find_dependency(Qt${QT_VERSION_MAJOR} REQUIRED COMPONENTS Widgets Core Gui PrintSupport)
#find_dependency(Qt5 5.12 COMPONENTS Widgets REQUIRED)

find_dependency(Threads)
find_dependency(LibXml2)

if(TARGET MVVM::Model OR TARGET MVVM::ViewModel OR TARGET MVVM::View)
else()
  message(STATUS "MVVMConfig.cmake including ${SUP_MVVM_CMAKE_DIR}/MVVMTargets.cmake")
 include("${SUP_MVVM_CMAKE_DIR}/SUP_MVVMTargets.cmake")
endif()

set(SUP_MVVM_LIBRARIES MVVM::Model MVVM::ViewModel MVVM::View)

message(STATUS "MVVMConfig.cmake in ${CMAKE_CURRENT_LIST_FILE}")
message(STATUS " MVVM_LIBRARIES ${SUP_MVVM_LIBRARIES}")


